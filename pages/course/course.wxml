<!--pages/miaowu/miaowu.wxml-->
<!-- 头部 -->
<view class="top">
  <image src="data:image/png;base64," class="log"></image>
  <view class="top-right">
    <image src="data:image/png;base64,"></image>
    <image src="data:image/png;base64,"></image>
  </view>
</view>
<!-- tab栏 -->
<view class="tab-box">
  <text bindtap="dyna" data-index="{{index}}" wx:for="{{tabdata}}" wx:key="index" class="{{index==id?'active':''}}">{{item}}</text>
</view>
<!-- 滑动模块 -->
<scroll-view scroll-x="true" wx:if="{{id == 0}}">
  <view class="hua-box">
    <view class="hua-item">
      <view class="img">
        <image src="../../img/话题榜单.png"></image>
      </view>
      <view class="title">···</view>
      <view class="num">查看更多考研话题</view>
    </view>
    <view class="hua-item" wx:for="{{themeList}}">
      <view class="img">
        <image src="{{item.cover}}"></image>
      </view>
      <view class="title">#{{item.title}}#</view>
      <view class="num">{{item.dyna_count}}讨论</view>
    </view>
  </view>
</scroll-view>
<!-- 发布 -->
<view wx:if="{{id != 1}}" class="release" wx:for="{{squarelist}}" wx:key="index">
  <view class="release-top">
    <view class="left">
      <!-- 头像 -->
      <view class="img">
        <image src="{{item.square_info.avatar}}" class="to"></image>
        <!-- <image src="/images/v.png" class="v"></image> -->
      </view>
      <view class="name-box">
        <!-- 账号名 -->
        <view>{{item.square_info.user_nickname}}</view>
        <!-- 日期 -->
        <view class="time">{{item.square_info.created_at}}</view>
      </view>
    </view>
    <!-- 关注 -->
    <view class="right">
      <view class="guan">
        +关注</view>
    </view>
    <!-- 三个点 -->
    <view class="dian">
      <text></text>
      <text></text>
      <text></text>
    </view>
  </view>
  <!-- 内容 -->
  <view class="substance">
    <view class="substance-title">{{item.square_info.dyna_details}}</view>
    <view class="substance-img">
    <view wx:for="{{item.square_info.dyna_cover}}"  wx:key="index">
       <image src="{{item}}"></image>
    </view>
      
    </view>
  </view>
  <!-- 尾部 -->
  <view class="footer">
    <view>
      <image src="../../img/分享.png"></image>
    </view>
    <view>
      <image src="../../img/评论.png"></image>
      <text>{{item.square_info.comment_count}}</text>
    </view>
    <view>
      <image src="../../img/猫爪.png"></image>
      <text>{{item.square_info.likes}}</text>
    </view>
  </view>
</view>
<!-- 定位 -->
<view class="miao"></view>
<!-- 图书勘误 -->
<view wx:if="{{id != 2}}" class="books" wx:for="{{newsList}}" wx:key="index">
   <!-- 头部 -->
   <view>
      <view class="left">
        <image src="{{item.avatar}}"></image>
        <view>
          <view>{{item.user_nickname}}</view>
          <view>{{item.updated_at}}</view>
        </view>
      </view>
      <view class="right">+关注</view>
   </view>
   <!-- 图书勘误区 -->
   <view class="h1">{{item.title}}</view>
   <!-- 图书勘误区 -->
   <!-- 内容 -->
   <view class="content">
      <image src="{{item.cover}}"></image>
      <view>
        {{item.content}}
        <text>...查看全文</text>
      </view>
   </view>
   <!-- 内容 -->
   <!-- 尾部 -->
   <view class="footer">
    <view>
      <image src="{{item.cover}}"></image>
    </view>
    <view>
      <image src="../../img/评论.png"></image>
      <text>{{item.comments}}</text>
    </view>
    <view>
      <image src="../../img/猫爪.png"></image>
      <text>{{item.likes}}</text>
    </view>
  </view>
   <!-- 尾部 -->
</view>
<!-- 图书勘误 -->
